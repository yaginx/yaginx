{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "LettuceEncrypt": "Verbose"
      }
    }
  },
  "AllowedHosts": "*",
  "DockerEngine": {
    "Socket": "unix:///var/run/docker.sock"
  },
  "RedisSettings": {
    "Host": "redis:6379",
    "Password": ""
  },
  "ConnectionStrings": {
    "Default": "filename=yaginx.db;password=yaginx;Mode=Exclusive"
  },
  "MongoSetting": {
    "Default": { "ConnectionString": "mongodb://mongodb:27017/yaginx" }
  }
}
