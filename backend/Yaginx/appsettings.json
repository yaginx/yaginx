{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "AgileLabs.WorkContexts": "Information",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.AspNetCore.Routing": "Debug",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "LettuceEncrypt": "Information",
        "Yaginx.YaginxAcmeLoaders": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "DockerEngine": {
    "Socket": "unix:///var/run/docker.sock"
  },
  "RedisSettings": {
    "Host": "redis:6379",
    "Password": ""
  },
  "ConnectionStrings": {
    "Default": "Server=pgdb;Port=5432;Database=yaginx;User Id=yaginx;Password=123456;Pooling=true;Application Name=Yaginx Apps;Include Error Detail=true;"
  },
  "MongoSetting": {
    "Default": { "ConnectionString": "mongodb://mongodb:27017/yaginx" }
  },
  "MonitorInterfacePattern": "" // 这里使用正则表达式, 空不做记录, 记录所有的是.号;
}
